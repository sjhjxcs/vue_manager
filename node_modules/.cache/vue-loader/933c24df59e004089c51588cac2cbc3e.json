cope\">\n          <span style=\"margin-left: 10px\"> {{ scope.row[item.prop] }}</span>\n        </template>\n      </el-table-column>\n      <el-table-column label=\"操作\" min-width=\"180\">\n        <template slot-scope=\"scope\">\n          <el-button size=\"min\" @click=\"handleEdit(scope.row)\">编辑</el-button>\n          <el-button size=\"min\" type=\"danger\" @click=\"handleDelete(scope.row)\"\n            >删除</el-button\n          >\n        </template>\n      </el-table-column>\n    </el-table>\n    <el-pagination\n      class=\"pager\"\n      layout=\"prev,pager,next\"\n      :total=\"config.total\"\n      :current-page.sync=\"config.page\"\n      @current-change=\"changePage\"\n      :page-size=\"20\"\n    ></el-pagination>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"CommonTable\",\n  props: {\n    tableData: Array,\n    tableLabel: Array,\n    config: Object,\n  },\n  data() {\n    return {};\n  },\n  methods: {\n    handleEdit(row) {\n      this.$emit(\"edit\", row);\n    },\n    handleDelete(row) {\n      this.$emit(\"del\", row);\n    },\n    changePage(page) {\n      this.$emit(\"changepage\", page);\n    },\n  },\n};\n</script>\n\n<style lang=\"less\" scoped>\n.common-table {\n  height: 500px;\n  background-color: #fff;\n  position: relative;\n  .pager {\n    position: absolute;\n    bottom: 0;\n    right: 20px;\n  }\n}\n</style>"]}]}